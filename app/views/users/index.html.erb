<h1>All Users</h1>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user do %>
        <%= user.firstname %> <%= user.familyname %>
      <% end %>
      , <%= user.email %> | <%= link_to "delete", user, method: :delete, data: { confirm: "You sure?" } %>
    </li>
  <% end %>
</ul>